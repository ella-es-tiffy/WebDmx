		jQuery.ajax({
	        url: ajaxurl,
	        type: 'POST',
	        data:{
	        	action: 'delete_enquiry_cart',
	          pro:id,
	        },
	        success:function(respons) {
	            if(respons){		
				
					jQuery('.header-cart-link #total-items').html(respons.all_num);
					jQuery('.widget_enquiry_content').html(respons.products);
					
					jQuery('.header-enquiry-dropdown').addClass('current-dropdown');
				}
	        },
	        error: function(errorThrown){
	            console.log(errorThrown);
	        }
	    });	
	}	
		
